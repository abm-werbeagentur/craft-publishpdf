{
    "name": "imhomedia/craft-publishpdf",
    "description": "Publish PDF files on Issuu or Yumpu",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "craft@imhomedia.at",
        "issues": "https://github.com/imhomedia/craft-publishpdf.git/issues?state=open",
        "source": "https://github.com/imhomedia/craft-publishpdf.git",
        "docs": "https://github.com/imhomedia/craft-publishpdf.git",
        "rss": "https://github.com/imhomedia/craft-publishpdf.git/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "imhomedia\\publishpdf\\": "src/"
        }
    },
    "extra": {
        "handle": "imhomedia-publishpdf",
        "name": "Publish PDF files",
        "developer": "Imhomedia",
        "documentationUrl": "https://github.com/imhomedia/craft-publishpdf.git"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

