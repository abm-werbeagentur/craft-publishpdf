{
    "name": "abmat/craft-publishpdf",
    "description": "Publish PDF files on Issuu or Yumpu",
    "type": "craft-plugin",
    "license": "proprietary",
	"authors": [
	  {
		"name": "ABM Feregyhazy & Simon GmbH",
		"homepage": "https://www.abm.at"
	  }
	],
    "support": {
        "email": "office@abm.at",
        "issues": "https://github.com/abm-werbeagentur/craft-publishpdf/issues?state=open",
        "source": "https://github.com/abm-werbeagentur/craft-publishpdf",
        "docs": "https://github.com/abm-werbeagentur/craft-publishpdf",
        "rss": "https://github.com/abm-werbeagentur/craft-publishpdf/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "abmat\\publishpdf\\": "src/"
        }
    },
    "extra": {
        "handle": "abmat-publishpdf",
        "name": "Publish PDF files",
        "developer": "ABM Feregyhazy & Simon GmbH",
        "documentationUrl": "https://github.com/abm-werbeagentur/craft-publishpdf"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

